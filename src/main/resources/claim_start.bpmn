<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="1.11.2">
  <bpmn:process id="Claim_Start" name="Provide initial claim information" isExecutable="true">
    <bpmn:documentation><![CDATA[Begin with this process when you want to start a new claim.  You will be asked for some basic information about the claimant and the policy.  Using the "discuss" and "assign" features, you can ask for assistance from others, including the policy holder.]]></bpmn:documentation>
    <bpmn:startEvent id="StartEvent_1">
      <bpmn:outgoing>SequenceFlow_0t4ad5t</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="SequenceFlow_0t4ad5t" sourceRef="StartEvent_1" targetRef="Task_12e3bo7" />
    <bpmn:userTask id="Task_12e3bo7" name="Information to initiate the claim process" camunda:formKey="steve-form-key">
      <bpmn:documentation>It is important that you provide complete and accurate information here.  Take the time to check your documents and records.</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="firstName" label="First Name" type="string" />
          <camunda:formField id="lastName" label="Last Name" type="string" />
          <camunda:formField id="lossDescription" label="Loss Description" type="string" />
          <camunda:formField id="cost_estimate" label="Cost Estimate" type="long" />
          <camunda:formField id="meta_CaseName" label="Name for this case" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>SequenceFlow_0t4ad5t</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_1v4tt50</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="SequenceFlow_1v4tt50" sourceRef="Task_12e3bo7" targetRef="Task_075fkdc" />
    <bpmn:userTask id="Task_075fkdc" name="Specify the type of claim" camunda:formKey="steve-form-key">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="claimType" label="What Type of Claim Is This?" type="enum">
            <camunda:value id="home" name="home" />
            <camunda:value id="auto" name="auto" />
            <camunda:value id="commercial" name="commercial" />
          </camunda:formField>
          <camunda:formField id="estimatedSeverity" label="Estimated Cost To Repair" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>SequenceFlow_1v4tt50</bpmn:incoming>
    </bpmn:userTask>
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Claim_Start">
      <bpmndi:BPMNShape id="_BPMNShape_StartEvent_2" bpmnElement="StartEvent_1">
