      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="SequenceFlow_0t4ad5t_di" bpmnElement="SequenceFlow_0t4ad5t">
        <di:waypoint xsi:type="dc:Point" x="209" y="120" />
        <di:waypoint xsi:type="dc:Point" x="287" y="120" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="248" y="105" width="0" height="0" />
